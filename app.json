{
  "expo": {
    "name": "ai-myshell",
    "slug": "yu-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "associatedDomains": [
        "pn564dc005-2f20-40a1-8082-c8634cdafd0b"
      ],
      "supportsTablet": true,
      "bundleIdentifier": "ai.myshell.app",
      "infoPlist": {
        "PROJECT_UUID": "c135c555-a871-4ec2-ac8c-5209ded4bfd1",
        "PROJECT_CLIENT_KEY": "clAJtavacSBZtWHNVrxYA8aXXk4dgO7azAMTd0eI",
        "PROJECT_APP_UUID": "564dc005-2f20-40a1-8082-c8634cdafd0b"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ai.myshell.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 31,
            "minSdkVersion": 23,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "@config-plugins/ffmpeg-kit-react-native",
        {
          "package": "min",
          "ios": {
            "package": "audio"
          }
        }
      ],
      [
        "./plugins/withAndroidNamespace.ts"
      ],
      [
        "./plugins/withAndroidGradeModifed.ts"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "./plugins/withAndroidMainActivityAttributes",
        {
          "android:PN_PROJECT_ID": "c135c555-a871-4ec2-ac8c-5209ded4bfd1",
          "android:PN_PROJECT_CLIENT_KEY": "clAJtavacSBZtWHNVrxYA8aXXk4dgO7azAMTd0eI",
          "android:PN_APP_ID": "bacfc13f-3b63-4d50-bb7a-2465999b8f7a"
        }
      ],
      [
        "./plugins/withiOSInfo"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "76605ade-0cad-4002-a878-350ea09f7d7b"
      }
    }
  }
}
