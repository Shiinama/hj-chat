{
  "cli": {
    "version": ">= 3.10.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development-simulator": {
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "dev": {
      "env": {
        "REACT_APP_ENV": "test"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "channel": "dev"
    },
    "test": {
      "env": {
        "REACT_APP_ENV": "test"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {},
      "channel": "test"
    },
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      },
      "channel": "development"
    },
    "preview": {
      "env": {
        "REACT_APP_ENV": "prod"
      },
      "android": {
        "channel": "preview",
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
